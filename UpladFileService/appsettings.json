{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5080",
        "PortServer": 5080
      }
    }
  },

  "Config": {
    "maximum_file_size_in_MB": 1024,
    "extension_fallback": "txt",
    "tmp_path": "wwwroot",
    "validity_period_days": 7,
    "extension_black_list": [
      ".exe",
      "html",
      "php"
    ]
  }
}
